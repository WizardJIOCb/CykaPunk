{
  "name": "cykapunk-backend",
  "version": "1.0.0",
  "description": "Backend server for CykaPunk game",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "node -r dotenv/config dist/db/migrate.js",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "game",
    "backend",
    "cyberpunk"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@esotericsoftware/spine-canvas": "^4.2.101",
    "@esotericsoftware/spine-core": "^4.2.101",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/passport": "^1.0.16",
    "@types/passport-discord": "^0.1.13",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-twitter": "^1.0.40",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.9",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-twitter": "^1.0.4",
    "pg": "^8.11.3",
    "socket.io": "^4.7.2",
    "typescript": "^5.3.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.16.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1"
  }
}
